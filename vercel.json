{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "headers": [
        {
        "source": "/icons/(.*).webp",
        "headers": [
            {
            "key": "Content-Type",
            "value": "image/webp"
            }
        ]
        },
        {
        "source": "/icons/(.*).png",
        "headers": [
            {
            "key": "Content-Type",
            "value": "image/png"
            }
        ]
        },
        {
        "source": "/media/(.*).jpeg",
        "headers": [
            {
            "key": "Content-Type",
            "value": "image/jpeg"
            }
        ]
        },
        {
        "source": "/media/(.*).svg",
        "headers": [
            {
            "key": "Content-Type",
            "value": "image/svg+xml"
            }
        ]
        }
    ],
    "rewrites": [
        {
        "source": "/icons/(.*)",
        "destination": "/public/icons/$1"
        },
        {
        "source": "/media/(.*)",
        "destination": "/public/media/$1"
        }
    ],    
    "routes": [
        { "src": "/build/(.*)", "dest": "/public/build/" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "public": true,
    "buildCommand": "vite build",
    "outputDirectory": "public",
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://yourproductionurl.com",
        "APP_KEY": "base64:TGaVaXwhWgjn9akhE4SCSIjGG/cQqvICUQ+PgLA3Bs0=",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "4gg.h.filess.io",
        "DB_PORT": "3306",
        "DB_DATABASE": "db2024_stormtorn",
        "DB_USERNAME": "db2024_stormtorn"
    }
}